{
  "version": 1,
  "author": "Mateus Alencar Ferreira",
  "editor": "wokwi",
  "parts": [
    { "type": "board-pi-pico-w", "id": "pico", "top": 0, "left": 0, "attrs": {} },
    {
      "type": "wokwi-led",
      "id": "led1",
      "top": 80,
      "left": -100,
      "attrs": { "color": "purple" }
    },
    {
      "type": "wokwi-resistor",
      "id": "r1",
      "top": 170,
      "left": -85,
      "attrs": { "value": "1000" }
    }
  ],
  "connections": [
    [ "pico:GP0", "$serialMonitor:RX", "", [] ],
    [ "pico:GP1", "$serialMonitor:TX", "", [] ],
    [ "pico:GP11", "led1:A", "green", [ "h0" ] ],
    [ "pico:GND.4", "r1:2", "black", [ "h0" ] ],
    [ "r1:1", "led1:C", "green", [ "v0" ] ]
  ],
  "dependencies": {}
}